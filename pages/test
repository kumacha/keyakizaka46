<template>
  <v-content>
    <v-container>
        <img id="first-img" v-bind:src="img_src" @mouseover="changeImg"
        @mouseleave="returnImg">
    </v-container>
  </v-content>
</template>

<script>
export default {
  methods: {
     changeImg (){
      documents.getElementById("first-img").addclass("originimage")
    },
    returnImg (){
      documents.getElementById("first-img").addclass("returnimage")
    }
  },
  data(){
    return{
      img_src: require("@/assets/image/slideshow1.jpg")
    }
    console.log(bigimage)
  }
}
</script>

<style>
img{
  border: dashed 1px silver;
  width: 300px;
  height: 500px;
  object-fit: cover;
}
.returnimage{
  width: 300px;
  height: 500px;
  object-fit: cover;
}
.originimage{
  width: 100%;
  height: 100%;
  object-fit: none;
}
</style>